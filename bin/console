#!/usr/bin/env php
<?php
// application.php

require __DIR__ . '/../vendor/autoload.php';

use App\CarMaster\Command\CarInformationCommand;
use App\CarMaster\Command\CreateCar;
use App\CarMaster\Command\CreateCarOwner;
use App\CarMaster\Command\CreateSparePart;
use App\CarMaster\Command\ExportCarOwner;
use App\CarMaster\Command\FindSparePartForVehicle;
use App\CarMaster\Command\RemoveSparePart;
use Symfony\Component\Console\Application;


$application = new Application();
$application->add(new CarInformationCommand());
$application->add(new FindSparePartForVehicle());
$application->add(new CreateSparePart());
$application->add(new CreateCarOwner());
$application->add(new CreateCar());
$application->add(new RemoveSparePart());
$application->add(new ExportCarOwner());
$application->add(new \App\Eloquent\Command\CreateCar());


// ... register commands
try {
    $application->run();
} catch (Exception $e) {
}

