#!/usr/bin/env php
<?php
// application.php

require __DIR__ . '/../vendor/autoload.php';

use App\Command\CreateCar;
use App\Command\CreateCarOwner;
use App\Command\CreateSparePart;
use App\Command\ExportCarOwner;
use App\Command\FindCarOwner;
use App\Command\FindSparePartForVehicle;
use App\Command\RemoveSparePart;
use Symfony\Component\Console\Application;
use App\Command\CarInformationCommand;


$application = new Application();
$application->add(new CarInformationCommand());
$application->add(new FindSparePartForVehicle());
$application->add(new CreateSparePart());
$application->add(new CreateCarOwner());
$application->add(new CreateCar());
$application->add(new RemoveSparePart());
$application->add(new FindCarOwner());
$application->add(new ExportCarOwner());


// ... register commands
try {
    $application->run();
} catch (Exception $e) {
}

